{
  "name": "dayify",
  "version": "1.0.0",
  "description": "Read human-friendly date formats",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "date",
    "parse",
    "calendar",
    "input",
    "moment",
    "luxon"
  ],
  "author": "Bill Wohlers",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/luxon": "^3.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "luxon": "^3.0.1"
  }
}
